{
    "name": "rovansteen/faker-object-factory",
    "type": "library",
    "require": {
        "php": "^8.1",
        "fakerphp/faker": "^1.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.12",
        "phpstan/phpstan": "^1.8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rovansteen\\FakerObjectFactory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Robert van Steen",
            "email": "hello@rovansteen.nl"
        }
    ],
    "scripts": {
        "format": "./vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "./vendor/bin/phpunit"
    }
}
